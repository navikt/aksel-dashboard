{
  "name": "dashboard",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts",
    "clean": "cd generator && gts clean",
    "test": "cd generator && jest",
    "g:dev": "yarn clean && cd generator &&  tsc -w",
    "g:build": "yarn clean && cd generator &&  tsc",
    "g:run": "cd generator && node  ./build/index.js",
    "g:run-f": "cd generator && FULL=true node --experimental-specifier-resolution=node ./build/index.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@types/node": "^14.11.2",
    "@types/react": "^18.0.17",
    "@types/semver": "7.3.12",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "gts": "3.1.1",
    "husky": "^8.0.0",
    "jest": "^29.1.2",
    "postcss": "^8.4.17",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.0",
    "vite": "^3.1.0",
    "@octokit/core": "4.0.5",
    "dotenv": "16.0.3",
    "@octokit/plugin-paginate-rest": "4.3.1",
    "simple-git": "3.14.1",
    "fast-glob": "3.2.12",
    "semver": "7.3.8"
  },
  "prettier": {}
}
