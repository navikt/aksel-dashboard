{
  "name": "dashboard",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "yarn workspace dashboard-website next dev",
    "build": "yarn workspace dashboard-website next build",
    "start": "yarn workspace dashboard-website next start",
    "lint": "yarn workspace dashboard-website next lint",
    "clean": "cd generator && gts clean",
    "g:dev": "yarn clean && cd generator &&  tsc -w",
    "g:build": "yarn clean && cd generator && tsc",
    "g:run": "cd generator && node  ./build/index.js",
    "g:run-f": "cd generator && FULL=true node --experimental-specifier-resolution=node ./build/index.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/semver": "7.3.12",
    "autoprefixer": "^10.4.12",
    "gts": "3.1.1",
    "husky": "^8.0.0",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "~4.7.0",
    "@octokit/core": "4.0.5",
    "dotenv": "16.0.3",
    "@octokit/plugin-paginate-rest": "4.3.1",
    "simple-git": "3.14.1",
    "fast-glob": "3.2.12",
    "semver": "7.3.8",
    "ts-morph": "16.0.0",
    "react-scanner": "1.1.0"
  },
  "workspaces": [
    "dashboard"
  ],
  "prettier": {}
}
